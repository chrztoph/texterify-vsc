{
    "name": "texterify",
    "displayName": "Texterify",
    "description": "An extension for VS Code to interact with Texterify.",
    "version": "0.0.1",
    "publisher": "texterify",
    "repository": {
        "type": "git",
        "url": "https://github.com/chrztoph/texterify-vsc"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:insert.key",
        "onCommand:download.keys"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "insert.key",
                "title": "Texterify: Insert Key"
            },
            {
                "command": "download.keys",
                "title": "Texterify: Download Keys"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "@types/lodash": "^4.14.109",
        "@types/nconf": "^0.0.37",
        "@types/shelljs": "^0.8.3",
        "lodash": "^4.17.10",
        "nconf": "^0.10.0",
        "shelljs": "^0.8.3",
        "tslint-microsoft-contrib": "^5.0.3"
    }
}
